    "lint-staged": "^10.0.7",
    "ngfe-request": "^1.0.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "umi": "^3.2.22",
    "umi-request": "^1.0.0",
    "xlsx": "^0.16.9",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "@umijs/plugin-qiankun": "^2.14.3"
